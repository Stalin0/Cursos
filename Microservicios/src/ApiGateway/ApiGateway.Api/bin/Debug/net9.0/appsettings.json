{
  "ReverseProxy": {
    "Routes": {
      "users": {
        "ClusterId": "users",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      },
      "products": {
        "ClusterId": "products",
        "Match": {
          "Path": "/products/{**catch-all}"
        }
      },
      "sales": {
        "ClusterId": "sales",
        "Match": {
          "Path": "/sales/{**catch-all}"
        }
      },
      "users-openapi": {
        "ClusterId": "users",
        "Match": {
          "Path": "/openapi/users/v1.json"
        },
        "Transforms": [
          {
            "PathPattern": "/openapi/v1.json"
          }
        ]
      },
      "products-openapi": {
        "ClusterId": "products",
        "Match": {
          "Path": "/openapi/products/v1.json"
        },
        "Transforms": [
          {
            "PathPattern": "/openapi/v1.json"
          }
        ]
      },
      "sales-openapi": {
        "ClusterId": "sales",
        "Match": {
          "Path": "/openapi/sales/v1.json"
        },
        "Transforms": [
          {
            "PathPattern": "/openapi/v1.json"
          }
        ]
      }
    },
    "Clusters": {
      "users": {
        "Destinations": {
          "usersDestination": {
            "Address": "http://localhost:5209/"
          }
        }
      },
      "products": {
        "Destinations": {
          "productsDestination": {
            "Address": "http://localhost:5174/"
          }
        }
      },
      "sales": {
        "Destinations": {
          "salesDestination": {
            "Address": "http://localhost:5005/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
